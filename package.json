{
  "name": "nodelearning",
  "version": "0.0.1",
  "description": "A learning sample for Node",
  "private": true,
  "scripts": {
    "prestart": "babel-node buildScripts/startMessage.js",
    "open:src": "babel-node buildScripts/srcServer.js",
    "lint": "esw webpack.config.* src buildScripts --color",
    "lint:watch": "npm run lint -- --watch",
    "start": "npm-run-all --parallel open:src lint:watch",
    "security-check": "nsp check",
    "build": "babel src -d lib",
    "test": "mocha --reporter progress buildScripts/testSetup.js \"src/**/*.spec.js\"",
    "test:watch": "npm run test -- --watch",
    "localtunnel": "npm-run-all --parallel open:src localtunnel"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/yvChauhan/NodeLearning.git"
  },
  "keywords": [
    "Learning",
    "node"
  ],
  "author": "Yash Chauhan",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/yvChauhan/NodeLearning/issues"
  },
  "homepage": "https://github.com/yvChauhan/NodeLearning#readme",
  "dependencies": {
    "chai": "^4.1.2",
    "chalk": "^2.4.1",
    "eslint": "^5.0.1",
    "eslint-plugin-import": "^2.13.0",
    "jsdom": "^11.11.0",
    "mocha": "^5.2.0",
    "npm-run-all": "^4.1.3",
    "npm-watch": "^0.3.0",
    "open": "0.0.5"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "css-loader": "^0.28.11",
    "express": "^4.16.3",
    "html-loader": "^0.5.5",
    "numeral": "^2.0.6",
    "webpack": "^4.14.0",
    "webpack-cli": "^3.0.8",
    "webpack-dev-middleware": "^3.1.3"
  }
}
